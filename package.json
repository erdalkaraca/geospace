{
  "name": "geospace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build-gslib": "esbuild src/app/rt/gs-lib.ts --outfile=public/lib/gs-lib.js --bundle --minify --format=esm",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:stats": "node scripts/test-stats.js",
    "test:ci": "npm run test:run && npm run test:coverage",
    "lint": "echo 'No linting configured yet'",
    "type-check": "tsc --noEmit",
    "ci": "npm run type-check && npm run test:ci",
    "validate-ci": "node scripts/validate-ci.js"
  },
  "dependencies": {
    "@awesome.me/webawesome": "^3.0.0-beta.6",
    "@fortawesome/fontawesome-free": "^6.7.2",
