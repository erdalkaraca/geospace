{
  "name": "geospace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=@kispace/geospace",
    "build": "npm run build --workspace=@kispace/geospace",
    "preview": "npm run preview --workspace=@kispace/geospace",
    "build-gslib": "npm run build-gslib --workspace=@kispace/geospace",
