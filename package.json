    "vite": "^6.3.1",
    "vite-plugin-cross-origin-isolation": "^0.1.6",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-static-copy": "^2.3.1",
    "vite-plugin-zip": "^1.0.1",
    "vitest": "^3.2.4"
  }
