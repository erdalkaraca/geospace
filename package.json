    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@lit-labs/signals": "^0.1.2",
    "@lit/context": "^1.1.5",
    "@mlc-ai/web-llm": "^0.2.79",
    "@xterm/xterm": "^5.5.0",
    "async-mutex": "^0.5.0",
    "axios": "^1.8.4",
    "dotenv": "^16.5.0",
    "esbuild-wasm": "^0.25.4",
    "font-gis": "^1.0.6",
    "idb-keyval": "^6.2.1",
    "jsonata": "^2.0.6",
    "jszip": "^3.10.1",
    "lit": "^3.2.1",
    "marked": "^15.0.11",
    "monaco-editor": "^0.52.2",
    "ol": "^10.5.0",
    "ol-mapbox-style": "^13.0.1",
    "osmtogeojson": "^3.0.0-beta.5",
    "pace-js": "^1.2.4",
    "pip-requirements-js": "^0.2.1",
    "pubsub-js": "^1.9.5",
    "pyodide": "^0.27.5",
    "signal-polyfill": "^0.2.2",
    "signal-utils": "^0.21.1",
    "toastify-js": "^1.12.0",
    "uuid": "^11.1.0",
    "wasm-git": "^0.0.13"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
